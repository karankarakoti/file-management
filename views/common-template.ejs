<!DOCTYPE html>
<html>
  <head>
    <title>
      <% if(title) { %>
        <% title %>
      <% } else { %>
        Common Template
      <% } %>
    </title>
    <link rel='stylesheet' href="/public/css/style.css" />        
    <link rel="icon" href="/public/images/favicon.png" type="image/x-icon" />
  </head>
  <body>
    <div class="welcome">
      <div class="welcome-box">
        <% if(title) { %>
          <h1><%= title %></h1>          
        <% } else { %>
          <h1></h1>>
        <% } %>
        <% if(desc) { %>          
          <p><%= desc %></p>        
        <% } else { %>
          <p></p>>
        <% } %>
        <div id="clock"></div>        
        <script src="/public/js/clock.js"></script>
      </div>   
      <div>
        <% if(brand) { %>                    
          <p style="text-align: center; box-sizing:border-box;margin-top:0;margin-bottom:10px;color:#6a737d!important;font-size:12px!important;font-family:-apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif,Apple Color Emoji,Segoe UI Emoji!important"><span class="il">
            <%= brand %>
          </p>     
        <% } else { %>
          <p></p>>
        <% } %>
      </div>   
    </div>  
  </body>
</html>
